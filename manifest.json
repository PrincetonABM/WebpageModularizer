{
	"manifest_version": 2,

	"name": "Modulr",
	"description": "Modularize Webpages",
	"version": "1.0",	
	"options_page": "options.html",
	
	"background": { 
		"scripts": ["init.js", "background.js"]
	},

	"browser_action": {
		"name": "Modularize the page",
		"default_icon": "images/testicon19.png"
	},


	"permissions": [
		"tabs",
        "storage",
		"http://*/*",
		"https://*/*"
	]

}   
